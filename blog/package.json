{
  "name": "hexo-site",
  "version": "0.0.0",
  "private": true,
  "hexo": {
    "version": "3.4.1"
  },
  "scripts": {
    "start": "hexo server --watch",
    "build": "hexo clean && hexo generate",
    "build:prod": "hexo clean && hexo generate --config _config.yml,_config.prod.yml && touch public/.static",
    "deploy": "hexo deploy"
  },
  "dependencies": {
    "hexo": "^3.2.0",
    "hexo-deployer-git": "^0.3.1",
    "hexo-featured-image": "^0.2.1",
    "hexo-generator-archive": "^0.1.4",
    "hexo-generator-category": "^0.1.3",
    "hexo-generator-feed": "^1.2.2",
    "hexo-generator-index": "^0.2.0",
    "hexo-generator-tag": "^0.2.0",
    "hexo-migrator-ghost": "^0.1.0",
    "hexo-prism-plugin": "^2.2.0",
    "hexo-render-pug": "^1.2.0",
    "hexo-renderer-ejs": "^0.3.0",
    "hexo-renderer-marked": "^0.3.0",
    "hexo-renderer-stylus": "^0.3.1",
    "hexo-server": "^0.2.0"
  }
}
