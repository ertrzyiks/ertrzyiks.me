# Example Terraform variables file
# Copy this file to terraform.tfvars and fill in your values
# terraform.tfvars is in .gitignore and will not be committed

dokku_ssh_host = "your-dokku-server.com"
dokku_ssh_user = "dokku"
dokku_ssh_port = 22

# For SSH certificate, you can use the file() function:
# dokku_ssh_cert = file("~/.ssh/id_rsa")
# Or directly paste your private key as a string (not recommended for production)
dokku_ssh_cert = "your-ssh-private-key"
