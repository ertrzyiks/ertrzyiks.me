{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": [
            "last 5 versions",
            "IE >= 11",
            "> 0.25%"
          ]
        },
        "useBuiltIns": "usage",
        "loose": true,
        "modules": false
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-syntax-dynamic-import",
    "@babel/plugin-transform-classes"
  ]
}
